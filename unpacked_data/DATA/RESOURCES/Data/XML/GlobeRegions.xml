<?xml version="1.0"?>
<GlobeRegions>



	<Region Name="Base_Region">
		<Behavior>
			REGION, 
			PRODUCTION, 
			GUI, 
			EFFECT_REGISTRY, 
			ATTRIBUTE_DIRECTORY
		</Behavior>
				
		<BehaviorType Name="GUIBehaviorType">
			<GUI_Scene_Name>Regional_Command_Center_Scene</GUI_Scene_Name>
			<GUI_Position_Automatically>true</GUI_Position_Automatically>
			<GUI_Receives_Input> yes </GUI_Receives_Input>
			<GUI_Min_Scale> 1.0 </GUI_Min_Scale>
			<GUI_Max_Scale> 1.0 </GUI_Max_Scale>
			<GUI_Default_Scale> 1.0 </GUI_Default_Scale>
			<GUI_Only_Service_When_Rendered> false </GUI_Only_Service_When_Rendered>
		</BehaviorType>
		
		
		<!-- Maria 02/2006: This ability allows a territory ton inflict damage on enemy fleets flying over it -->
		<Abilities SubObjectList="Yes">
			<Hostile_Territory_Ability Name="Hostile_Territory">
			<Initially_Enabled>Yes</Initially_Enabled>
			<Activation_Style>Global_Automatic</Activation_Style>
			
			<!-- Value between 0 and 0.999 which represents by how much we will decrease the hostile fleet's speed -->
			<Hostile_Fleet_Speed_Modifier> 0.0 </Hostile_Fleet_Speed_Modifier>  
			<Damage_To_Hostile_Fleet_Modifier> -1.0 </Damage_To_Hostile_Fleet_Modifier>
			</Hostile_Territory_Ability>
		</Abilities>
		
				
	</Region>


