<?xml version="1.0" ?>
<!-- Dynamic enum definition for GameObjectCategoryType  -->
<EnumDefinition>

	<!-- This is a bitfield! If we hit the 32 entry barrier, I'll look into support 64 bit enums -->


	<!-- Size Categories -->

		<Small></Small>				<!-- Use for Infantry -->
		<Medium></Medium>			<!-- Use for normal sized vehicles, small structures -->
		<Large></Large>				<!-- Use for large vehicles or structures -->
		<Huge></Huge>				<!-- Use for walkers, skyscrapers -->



	<!-- Locomotion Categories -->

		<Flying></Flying>
		<Stationary></Stationary>	<!-- Used for structures -->



	<!-- Composition Categories -->

		<Organic></Organic>			<!-- Infantry and large living creatures (like brutes) -->
		<Piloted></Piloted>			<!-- Vehicles -->
		<Resource></Resource>
      <Resource_INST></Resource_INST>
      <Superweapon></Superweapon>
      <CanAttack></CanAttack>
		<Hero></Hero>
		<Bridge></Bridge>
		<Male></Male><!-- for some unit effects, we need to know the gender (s.a. which SFX to apply to target) -->
		<Female></Female><!-- for some unit effects, we need to know the gender (s.a. which SFX to apply to target) -->

		<HardPoint></HardPoint>
      <LegSocketHP></LegSocketHP>
		<BodySocketHP></BodySocketHP>
	
		<TeamContainer></TeamContainer>

		<DeathCloneResource></DeathCloneResource>  <!-- this category is automatically added by death behavior to all death clones with Attribute_Directory -->
	
	<!-- Effect Categories -->
	
		<RadiationHelps></RadiationHelps>
		<RadiationHurts></RadiationHurts>
   	
	<!-- Auto-Targeting Categories -->
	
		<Insignificant></Insignificant>	<!-- Only heroes can autonomously target these. -->
		<Untargetable></Untargetable>
		<DummyTarget></DummyTarget>



	<!-- Special Case Categories -->

		<!-- This object is under CRUSH attack and cannot be taregted by attacker's hardpoints -->
		<!-- I know it's a waste, but can't find another way to do this. -Oksana  -->
		<UnderCrushAttack></UnderCrushAttack>

      <!-- Same case as above. The unit that applies Destabilize effect cannot target destabilized units. -Oksana  -->
      <UnderDestabilizeEffect></UnderDestabilizeEffect>

		<TacticalBuildableStructure></TacticalBuildableStructure>
	   <StoryTargetObject></StoryTargetObject>

   
		<All>	0xFFFFFFFF	</All>
		<None>	0x00000000	</None>



</EnumDefinition>

