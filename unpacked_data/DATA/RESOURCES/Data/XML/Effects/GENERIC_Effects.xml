      <Extends_Existing_Duration> yes </Extends_Existing_Duration>

      <Effect_Component Type="DisableBehavior">
         <Behaviors> TURRET, WEAPON, TACTICAL_ENABLER, TARGETING, RADIATION_WEAPON </Behaviors>
