   <Effect Name="MasariArchitectAttackBetterEffect">
      <Effect_Class> AssistStructureImproveAttack </Effect_Class>
      <Extends_Existing_Duration> true </Extends_Existing_Duration>
      <Extends_Same_Source_Duration_Only> true </Extends_Same_Source_Duration_Only>

      <Effect_Component Type="AttributeModifier">
         <Attribute> Number_Of_Active_Assistants </Attribute>
         <Modifier_Value> 1.0 </Modifier_Value>
      </Effect_Component>

      <Effect_Component Type="AttributeModifier">
         <Attribute> Weapon_Rate_Multiplier </Attribute>
			<!-- KDB removed negative delay as that was causing a werd ROF (and DOT) curve. -->
