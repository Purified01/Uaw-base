<?xml version="1.0"?>

<VibrationEvents>

	<!-- ================== -->
	<!-- Begin Test Events  -->
	<!-- ================== -->

	<VibrationEvent Name="Test_Left_Motor_Hold">

		<Priority> 10 </Priority>	<!-- Priorities can be 1-10 or higher - Priority 1 is a top priority -->
		<Is_Ambient> Yes </Is_Ambient>
		<Is_Notification> Yes </Is_Notification>

		<Left_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_HOLD, 50, 3000, <!-- Hold 50% for 3 seconds (no ramp up) -->
			MOTOR_COMMAND_END
		</Left_Motor_Script>

	</VibrationEvent>

	<VibrationEvent Name="Test_Right_Motor_Hold">

		<Priority> 9 </Priority>  <!-- Priorities can be 1-10 or higher - Priority 1 is a top priority -->
		<Is_Ambient> Yes </Is_Ambient>
		<Is_Notification> Yes </Is_Notification>

		<Right_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_HOLD, 50, 3000, <!-- Hold 50% for 3 seconds (no ramp up)  -->
			MOTOR_COMMAND_END
		</Right_Motor_Script>

	</VibrationEvent>

	<VibrationEvent Name="Test_Left_Motor_Ramp">

		<Priority> 8 </Priority>  <!-- Priorities can be 1-10 or higher - Priority 1 is a top priority -->
		<Is_Ambient> Yes </Is_Ambient>
		<Is_Notification> Yes </Is_Notification>

		<Left_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_RAMP, 0, 100, 5000, <!-- Ramp 0% to 100% over 5 seconds -->
			MOTOR_COMMAND_RAMP, 100, 0, 5000, <!-- Ramp 100% to 0% over 5 seconds -->
			MOTOR_COMMAND_END
		</Left_Motor_Script>

	</VibrationEvent>

	<VibrationEvent Name="Test_Right_Motor_Ramp">

		<Priority> 7 </Priority>  <!-- Priorities can be 1-10 or higher - Priority 1 is a top priority -->
		<Is_Ambient> Yes </Is_Ambient>
		<Is_Notification> Yes </Is_Notification>

		<Right_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_RAMP, 0, 100, 5000, <!-- Ramp 0% to 100% over 5 seconds --> 
			MOTOR_COMMAND_RAMP, 100, 0, 5000, <!-- Ramp 100% to 0% over 5 seconds --> 
			MOTOR_COMMAND_END
		</Right_Motor_Script>

	</VibrationEvent>

	<VibrationEvent Name="Test_Left_Motor_Loop">

		<Priority> 6 </Priority>  <!-- Priorities can be 1-10 or higher - Priority 1 is a top priority -->
		<Is_Ambient> Yes </Is_Ambient>
		<Is_Notification> Yes </Is_Notification>

		<Left_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_RAMP, 0, 100, 1000, <!-- Ramp 0% to 100% over 1 second --> 
			MOTOR_COMMAND_RAMP, 100, 0, 1000, <!-- Ramp 100% to 0% over 1 second --> 
			MOTOR_COMMAND_LOOP,					 <!-- Repeat loop -->  		 
			MOTOR_COMMAND_END
		</Left_Motor_Script>

	</VibrationEvent>

	<VibrationEvent Name="Test_Right_Motor_Loop">

		<Priority> 5 </Priority>  <!-- Priorities can be 1-10 or higher - Priority 1 is a top priority -->
		<Is_Ambient> Yes </Is_Ambient>
		<Is_Notification> Yes </Is_Notification>

		<Right_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_RAMP, 0, 100, 1000, <!-- Ramp 0% to 100% over 1 second -->
			MOTOR_COMMAND_RAMP, 100, 0, 1000, <!-- Ramp 100% to 0% over 1 second -->
			MOTOR_COMMAND_LOOP,					 <!-- Repeat loop -->                    		 
			MOTOR_COMMAND_END
		</Right_Motor_Script>

	</VibrationEvent>

	<VibrationEvent Name="Test_Dual_Motors_Cross_Ramp">

		<Priority> 4 </Priority>  <!-- Priorities can be 1-10 or higher - Priority 1 is a top priority -->
		<Is_Ambient> Yes </Is_Ambient>
		<Is_Notification> Yes </Is_Notification>

		<Left_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_RAMP, 0, 100, 10000, <!-- Ramp 0% to 100% over 10 seconds --> 
			MOTOR_COMMAND_RAMP, 100, 0, 10000, <!-- Ramp 100% to 0% over 10 seconda --> 
			MOTOR_COMMAND_END
		</Left_Motor_Script>

		<Right_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_RAMP, 100, 0, 10000, <!-- Ramp 100% to 0% over 10 seconds -->  
			MOTOR_COMMAND_RAMP, 0, 100, 10000, <!-- Ramp 0% to 100% over 10 seconda -->  
			MOTOR_COMMAND_END
		</Right_Motor_Script>

	</VibrationEvent>

	<VibrationEvent Name="Test_Dual_Motors_Loop">

		<Priority> 3 </Priority>  <!-- Priorities can be 1-10 or higher - Priority 1 is a top priority -->
		<Is_Ambient> Yes </Is_Ambient>
		<Is_Notification> Yes </Is_Notification>

		<Left_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_RAMP, 0, 100, 10000, <!-- Ramp 0% to 100% over 10 seconds -->  
			MOTOR_COMMAND_RAMP, 100, 0, 10000, <!-- Ramp 100% to 0% over 10 seconda -->  
			MOTOR_COMMAND_LOOP,					  <!-- Repeat loop -->                    		 
			MOTOR_COMMAND_END
		</Left_Motor_Script>

		<Right_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_RAMP, 100, 0, 500, <!-- Ramp 100% to 0% over .5 seconds --> 
			MOTOR_COMMAND_RAMP, 0, 100, 500, <!-- Ramp 0% to 100% over .5 seconda --> 
			MOTOR_COMMAND_LOOP,	 				<!-- Repeat loop -->                    		   						 
			MOTOR_COMMAND_END
		</Right_Motor_Script>

	</VibrationEvent>

	<VibrationEvent Name="Test_Motors_No_Effect">

		<Priority> 2 </Priority>  <!-- Priorities can be 1-10 or higher - Priority 1 is a top priority -->
		<Is_Ambient> Yes </Is_Ambient>
		<Is_Notification> Yes </Is_Notification>
 
		<Left_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_END
		</Left_Motor_Script>

		<Right_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_END
		</Right_Motor_Script>

	</VibrationEvent>

	<!-- ================ -->
	<!-- End Test Events  -->
