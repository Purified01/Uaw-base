	<!-- ==================================================== -->
	<!-- Event for a high-priority game event (called in the misc GameEventNotifications) -->
	<!-- HIERARCHY_GameEventNotification.xml -->
	<!-- MASARI_GameEventNotification.xml -->
	<!-- NOVUS_GameEventNotification.xml -->
	<!-- MILITARY_GameEventNotification.xml -->
	<!-- ==================================================== -->
	
	<VibrationEvent Name="Vibration_GameEvent_HighPriority">

		<Priority> 1 </Priority>  <!-- Priorities can be 1-10 or higher - Priority 1 is a top priority -->
		<Is_Ambient> No </Is_Ambient>
		<Is_Notification> Yes </Is_Notification>

		<Right_Motor_Script>
			MOTOR_COMMAND_INIT,
			MOTOR_COMMAND_HOLD, 100, 500,  <!-- jdg .. holds at 100% for 1/2 sec then gives -->
			MOTOR_COMMAND_HOLD, 0, 200,  		<!-- 3 quick buzzes... -->
			MOTOR_COMMAND_HOLD, 100, 100,  	<!-- should feel artificial to catch peoples attention -->
			MOTOR_COMMAND_HOLD, 0, 200,  
			MOTOR_COMMAND_HOLD, 100, 100,  
			MOTOR_COMMAND_HOLD, 0, 200,  
			MOTOR_COMMAND_HOLD, 100, 200,  
			MOTOR_COMMAND_END
		</Right_Motor_Script>

	</VibrationEvent>
	
