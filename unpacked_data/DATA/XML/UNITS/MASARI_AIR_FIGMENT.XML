   <!--=================================================================================================
	== [10/17/2006] Base object for Figment's mines - Oksana Kubushyna
	================================================================================================== -->
   <X13_Masari_Units Name="Masari_Figment_Elemental_Mine">

      <!-- ART -->
      <Scale_Factor>1.0</Scale_Factor>
      <Is_Decoration>No</Is_Decoration>

      <!-- INTERFACE -->
		<Text_ID>TEXT_NONE</Text_ID>
		<Is_Placed_By_Editor> false </Is_Placed_By_Editor>

      <!-- ATTRIBUTES -->
      <Is_Immune_To_Tornado>true</Is_Immune_To_Tornado>
		<Victory_Relevant> No </Victory_Relevant>
		<CategoryMask> Small, Insignificant, RadiationHurts </CategoryMask>

		<Attributes SubObjectList="Yes">
         <!-- DO NOT CHANGE the tag (you can change the value)!
			     LUA targeting function for elemental mines relies on this for radius. -Oksana -->
         <Generator_Area_Effect_Radius_1> 80 </Generator_Area_Effect_Radius_1>
			<Is_Immune_To_Virus> true </Is_Immune_To_Virus>
			<Is_Immune_To_Mind_Control> true </Is_Immune_To_Mind_Control>
      </Attributes>

      <!-- CONFLICT -->
      <Ranged_Target_Z_Adjust>3.0</Ranged_Target_Z_Adjust>
		<!-- KDB High health as this will destroy iteself if damaged -->
      <Tactical_Health> 450.0 </Tactical_Health>
		<Is_Attackable>true</Is_Attackable>
		<Collidable_By_Projectile_Living>Yes</Collidable_By_Projectile_Living>

		<!-- BEHAVIORS -->
      <LandBehavior>
         EFFECT_REGISTRY,
         EFFECT_GENERATOR,
         ATTRIBUTE_DIRECTORY,
         BALLISTIC,
         HIDE_WHEN_FOGGED, <!-- we need this so that CLOCK hides the model properly for the enemy player-->
         REVEAL <!-- we need this so that friendly can still see us when we're cloaked-->
      </LandBehavior>

      <BehaviorType Name="BallisticBehaviorType">
         <Ballistic_Weight>12.0</Ballistic_Weight>
         <Ballistic_Bounce>0.25</Ballistic_Bounce>
      </BehaviorType>

      <BehaviorType Name="ArmorAndDamageBehaviorType">
         <Armor_Type> Armor_Default </Armor_Type>
      </BehaviorType>

		<BehaviorType Name="ProductionBehaviorType">
			<Game_Score_Unit> false </Game_Score_Unit>
		</BehaviorType>

		<BehaviorType Name="DeathFXBehaviorType">
         <Remove_Upon_Death>Yes</Remove_Upon_Death>
      </BehaviorType>

      <Effect_Generator_Data SubObjectList="Yes">

         <!-- Mine falls down on the ground -->
         <Effect_Generator Name="MasariFigmentMineFallDownEffectGenerator">
            <Should_Deactivate_Manually> true </Should_Deactivate_Manually>
            <Starts_Active> true </Starts_Active>

				<Effect_Generator_Entry Name="Entry1">
					<Effect_Name> MineInvulnerabilityEffect </Effect_Name>
					<Duration> 2.0 </Duration>
					<Target_Gathering_Mask> Self </Target_Gathering_Mask>
				</Effect_Generator_Entry>

				<!-- Mines need to fall down when spawned up in the air -->
            <Effect_Generator_Entry Name="Entry2">
               <Effect_Name> LaunchBallisticBehaviorEffect </Effect_Name>
               <!-- should not remove itself until destroyed -->
               <Duration>-1</Duration>
               <Target_Gathering_Mask> Self </Target_Gathering_Mask>
            </Effect_Generator_Entry>
         </Effect_Generator>

         <!-- Mine cloaks itself over time -->
         <Effect_Generator Name="MasariFigmentMineEffectGenerator">
            <Should_Deactivate_Manually> true </Should_Deactivate_Manually>
            <!-- If revealed, mine should re-cloak promptly -->
            <Modal_Refresh_Frequency>3.0</Modal_Refresh_Frequency>
            <Starts_Active> true </Starts_Active>

            <Effect_Generator_Entry Name="Entry1">
               <Effect_Name> MasariElementalMineCloakEffect </Effect_Name>
               <!-- will stay cloaked until destroyed -->
               <Duration>-1</Duration>
               <Base_Delay>5</Base_Delay>
               <!-- wait some time before cloaking so that enemies have a chance to kill it -->
               <Target_Gathering_Mask> Self </Target_Gathering_Mask>
            </Effect_Generator_Entry>

            <Effect_Generator_Entry Name="Entry2">
               <Effect_Name> DebugFireMineShowRadiusEffect </Effect_Name>
               <Duration>-1</Duration>
               <Target_Gathering_Mask> Self </Target_Gathering_Mask>
            </Effect_Generator_Entry>

         </Effect_Generator>


         <!-- Mine detonates by enemy proximity -->
         <Effect_Generator Name="MasariFigmentMineDetonationTriggerEffectGenerator">
            <Should_Deactivate_Manually> true </Should_Deactivate_Manually>
            <Starts_Active> true </Starts_Active>
            <!-- Scan for enemies periodically -->
            <Modal_Refresh_Frequency>0.5</Modal_Refresh_Frequency>
            <Effect_Generator_Entry Name="Entry1">
               <Duration>1.0</Duration>
               <Effect_Name> MasariElementalMineAnnounceDetonationEffect </Effect_Name>
               <Target_Gathering_Mask> Self </Target_Gathering_Mask>
               <!-- the radius of the effect is in the LUA function, uses Generator_Area_Effect_Radius_1 attribute -->
               <Target_Filter_C>Masari_Elemental_Mine_Detonation</Target_Filter_C>
            </Effect_Generator_Entry>

         </Effect_Generator>

      </Effect_Generator_Data>

   </X13_Masari_Units>

