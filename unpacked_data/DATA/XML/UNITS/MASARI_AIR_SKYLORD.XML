			<Effect_Generator Name="Masari_Skylord_Screech_Attack_Generator">
				<Should_Deactivate_Manually>No</Should_Deactivate_Manually>

				<Effect_Generator_Entry Name="Entry1">
					<Effect_Name> SkylordScreechStunEffect </Effect_Name>
					<Duration> 6.0 </Duration>
					<Target_Gathering_Mask> SelfAE </Target_Gathering_Mask>
					<Area_Effect_Radius> 200.0 </Area_Effect_Radius>
					<Target_Filter_Categories>Flying</Target_Filter_Categories>
