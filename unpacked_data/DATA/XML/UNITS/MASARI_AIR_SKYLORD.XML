				<Effect_Generator_Entry Name="Entry2">
					<Effect_Name> SkylordScreechEffect </Effect_Name>
					<Duration> 0.1 </Duration>
					<Target_Gathering_Mask> SelfAE </Target_Gathering_Mask>
					<Area_Effect_Radius> 200.0 </Area_Effect_Radius>
					<Target_Filter_Categories>Flying</Target_Filter_Categories>
					<Target_Filter_C> Is_Enemy </Target_Filter_C>
				</Effect_Generator_Entry>

				<Effect_Generator_Entry Name="Entry3">
					<Effect_Name> SkylordScreechOwnerEffect </Effect_Name>
					<Duration> 0.1 </Duration>
					<Target_Gathering_Mask> Self </Target_Gathering_Mask>
				</Effect_Generator_Entry>



			</Effect_Generator>

			<!-- Oksana [05/21/2007]: Spawn corresponding Phoenix on Death - must precede Avenger generator -->
			<!-- Phoenix (Passive Researched, Offense mode): When destroyed in offense mode, the unit  
                 has a chance of being reborn and returning to the sky at half maximum health. -->
			<Effect_Generator Name="MasariSpawnPhoenixOnDeathGenerator">
				<Starts_Active>Yes</Starts_Active>
				<Modal_Refresh_Frequency>1</Modal_Refresh_Frequency>
				<Effect_Generator_Entry Name="Entry1">
					<Duration> 1.1 </Duration>
					<Effect_Name> MasariPhoenixEffect </Effect_Name>
					<Target_Gathering_Mask> Self </Target_Gathering_Mask>
					<Target_Application_Probability>0.5</Target_Application_Probability>
					<Target_Filter_C> Can_Create_Phoenix </Target_Filter_C>
					<Remove_On_Death>No</Remove_On_Death>
				</Effect_Generator_Entry>
			</Effect_Generator>

			<!-- If I am a Phoenix, I should take 50% damage when created!-->
			<Effect_Generator Name="MasariPhoenixDamageGenerator">
				<Starts_Active>Yes</Starts_Active>
				<Self_Start_Delay>0.5</Self_Start_Delay>

				<Effect_Generator_Entry Name="Entry1">
					<Effect_Name> MasariPhoenixDamageEffect </Effect_Name>
					<Target_Gathering_Mask> Self </Target_Gathering_Mask>
					<Target_Filter_C> Is_Phoenix </Target_Filter_C>
				</Effect_Generator_Entry>
			</Effect_Generator>

			<!-- If I am a Phoenix, I should play nice animation when created!-->
			<Effect_Generator Name="MasariPhoenixVisualGenerator">
				<Starts_Active>Yes</Starts_Active>
				<Self_Start_Delay>0.1</Self_Start_Delay>

				<Effect_Generator_Entry Name="Entry1">
					<Duration>2</Duration>
					<Effect_Name> MasariPhoenixVisualEffect </Effect_Name>
					<Target_Gathering_Mask> Self </Target_Gathering_Mask>
					<Target_Filter_C> Is_Phoenix </Target_Filter_C>
					<Deactivate_Parent_When_Idle>Yes</Deactivate_Parent_When_Idle>
				</Effect_Generator_Entry>
			</Effect_Generator>

			<!-- Oksana [05/21/2007]: Spawn corresponding Avenger on Death -->
			<Effect_Generator Name="MasariSpawnAvengerOnDeathGenerator">
				<Starts_Active>Yes</Starts_Active>

				<Effect_Generator_Entry Name="Entry1">
					<Duration> -1.0 </Duration>
					<Effect_Name> MasariSpawnAvengerOnDeathEffect </Effect_Name>
					<Target_Gathering_Mask> Self </Target_Gathering_Mask>
				</Effect_Generator_Entry>
			</Effect_Generator>

			<!-- Standard Sell generator  - 06/28/2007 -->
			<!--INCLUDE ".\Data\XML\Includes\Sell_Effect_Generators\MASARI_Sell_Generators.xml" -->

		</Effect_Generator_Data>

