	<X90_Other_Objects Name="Masari_Clear_Mind_Control_Switch_Object">
		<Is_Placed_By_Editor>false</Is_Placed_By_Editor>
		<LandBehavior>MARKER, EFFECT_REGISTRY, EFFECT_GENERATOR, ATTRIBUTE_DIRECTORY</LandBehavior>
