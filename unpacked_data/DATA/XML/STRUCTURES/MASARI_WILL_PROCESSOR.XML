			TACTICAL_BUILD_OBJECTS,
			GUI,
			RADAR_MAP_VISIBLE,
			REVEAL,
			HIDE_WHEN_FOGGED,
			UNIT_AI,
			AI_RECRUITABLE,
			AI_TARGET,
			IDLE,
			SWITCH_TYPE,
			TERRAFORM,
