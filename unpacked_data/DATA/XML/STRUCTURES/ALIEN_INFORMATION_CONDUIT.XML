      <Remove_Upon_Death>true</Remove_Upon_Death>
    </BehaviorType>

		<BehaviorType Name="CrushBehaviorType">
			<Crush_Defense>90</Crush_Defense>
		</BehaviorType>

  </X10_Alien_Structures>

</Alien_Information_Conduit_XML>
