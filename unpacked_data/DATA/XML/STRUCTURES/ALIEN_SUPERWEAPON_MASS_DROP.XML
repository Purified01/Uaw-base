            <Type> Alien_Superweapon_Mass_Drop_Air_Proxy </Type>
            <Creation_Trigger> OnParentBehaviorFirstService </Creation_Trigger>
            <Destruction_Trigger> OnParentBehaviorRemove </Destruction_Trigger>
         </Create_Child_Data>

