  <Effect Name="Military_Builder_Build_Effect">
    <Duration> 0.0 </Duration>
    <Effect_Class> TacticalBuild </Effect_Class>

    <!-- The more builders per object, the faster the build -->
    <!-- max of 10 builders 0.2 x (11-1) = 2 can help -->
